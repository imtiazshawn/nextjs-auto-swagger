{
  "name": "nextjs-auto-swagger",
  "version": "1.0.1",
  "description": "Automatic Swagger UI generation for Next.js 15+ App Router API routes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "next.js",
    "swagger",
    "swagger-ui",
    "openapi",
    "api",
    "api-documentation",
    "app-router",
    "nextjs15",
    "typescript",
    "nextjs-swagger",
    "nextjs-openapi"
  ],
  "author": "Imtiaz Shawn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imtiazshawn/nextjs-auto-swagger"
  },
  "bugs": {
    "url": "https://github.com/imtiazshawn/nextjs-auto-swagger/issues"
  },
  "homepage": "https://github.com/imtiazshawn/nextjs-auto-swagger#readme",
  "peerDependencies": {
    "next": ">=15.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "swagger-ui-dist": "^5.10.0"
  }
}